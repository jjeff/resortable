{
  "$schema": "https://typedoc.org/schema.json",
  "name": "Resortable",
  "entryPoints": ["src/index.ts"],
  "out": "docs",
  "theme": "default",
  "plugin": ["typedoc-plugin-markdown"],
  "gitRevision": "main",
  "gitRemote": "origin",
  "readme": "README.md",
  "includeVersion": true,
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeExternals": true,
  "excludeInternal": false,
  "categorizeByGroup": true,
  "defaultCategory": "Other",
  "categoryOrder": [
    "Core",
    "Animation",
    "Plugins", 
    "Utils",
    "Types",
    "Other"
  ],
  "sort": ["source-order"],
  "kindSortOrder": [
    "Document",
    "Project",
    "Module",
    "Namespace",
    "Enum",
    "EnumMember",
    "Class",
    "Interface",
    "TypeAlias",
    "Constructor",
    "Property",
    "Variable",
    "Function",
    "Accessor",
    "Method",
    "Parameter",
    "TypeParameter",
    "TypeLiteral",
    "CallSignature",
    "ConstructorSignature",
    "IndexSignature",
    "GetSignature",
    "SetSignature"
  ],
  "navigation": {
    "includeCategories": true,
    "includeGroups": true
  },
  "searchInComments": true,
  "searchInDocuments": true,
  "cleanOutputDir": true,
  "treatWarningsAsErrors": false,
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": false
  },
  "commentStyle": "jsdoc",
  "blockTags": [
    "@deprecated",
    "@param",
    "@returns",
    "@throws",
    "@example",
    "@remarks",
    "@see",
    "@since",
    "@version",
    "@author",
    "@internal",
    "@beta",
    "@alpha",
    "@experimental"
  ],
  "inlineTags": [
    "@link",
    "@linkcode",
    "@linkplain",
    "@inheritDoc"
  ],
  "modifierTags": [
    "@override",
    "@virtual",
    "@sealed",
    "@deprecated",
    "@experimental",
    "@internal",
    "@alpha",
    "@beta"
  ]
}